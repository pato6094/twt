{
   "author": "Alexander Choporov (CoolCmd)",
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "exclude_matches": [ "https://www.twitch.tv/popout/" ],
      "js": [ "autoclaim.js" ],
      "matches": [ "https://www.twitch.tv/*" ],
      "run_at": "document_idle"
   }, {
      "all_frames": true,
      "js": [ "gqltoken.js" ],
      "matches": [ "https://www.twitch.tv/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "exclude_matches": [ "https://www.twitch.tv/popout/" ],
      "js": [ "common.js", "content.js" ],
      "matches": [ "https://www.twitch.tv/*", "https://m.twitch.tv/*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
   },
   "default_locale": "en",
   "description": "__MSG_M0001__",
   "icons": {
      "128": "manifest.png"
   },
   "incognito": "split",
   "key": "",
   "manifest_version": 3,
   "name": "Alternate Player for Twitch.tv",
   "optional_permissions": [ "contentSettings" ],
   "permissions": [ "storage", "cookies", "declarativeNetRequest", "declarativeNetRequestWithHostAccess", "management", "system.memory", "system.cpu" ],
   "host_permissions": [
      "*://*.twitch.tv/*",
      "*://*.twitchcdn.net/*",
      "*://*.ttvnw.net/*",
      "*://*.jtvnw.net/*",
      "*://*.live-video.net/*",
      "*://*.akamaized.net/*",
      "*://*.cloudfront.net/*"
   ],
   "short_name": "Alternate Player",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2025.6.16",
   "web_accessible_resources": [{
      "resources": [ "player.html", "content.css" ],
      "matches": [ "<all_urls>" ]
   }]
}
